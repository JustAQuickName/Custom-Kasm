{
  "description": "draw.io is free online diagram software.",
  "docker_registry": "https://index.docker.io/v1/",
  "image_src": "drawio.png",
  "name": "jgraph/drawio",
  "run_config": {
    "hostname": "kasm"
  },
  "exec_config": {
    "go": {
      "cmd": "bash -c '/dockerstartup/start.sh --go --url \"$KASM_URL\"'"
    },
    "assign": {
      "cmd": "bash -c '/dockerstartup/start.sh --assign --url \"$KASM_URL\"'"
    }
  },
  "categories": [
    "Diagram Software"
  ],
  "friendly_name": "Draw.io",
  "architecture": [
    "amd64",
    "arm64"
  ],
  "compatibility": [
    "1.13.x",
    "1.14.x"
  ],
  "uncompressed_size_mb": 2000
}
